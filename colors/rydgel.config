## Termite Styling Program
## Author : Aditya Shakya

[options]

allow_bold = true
audible_bell = true
browser = xdg-open
clickable_url = true
cursor_blink = system
cursor_shape = block
dynamic_title = true
filter_unmatched_urls = true
font = Iosevka Nerd Font 10
geometry = 480x280
icon_name = terminal
modify_other_keys = false
mouse_autohide = false
scroll_on_output = false
scroll_on_keystroke = true
scrollback_lines = 15000
size_hints = false
search_wrap = true
urgent_on_bell = true
highlight = #30343e

[colors]

# Black
color0=   #303430
color8=   #cdb5cd

# Red
color1=   #bf7979
color9=   #f4a45f

# Green
color2=   #97b26b
color10=  #c5f779

# Yellow
color3=   #cdcdc1
color11=  #ffffed

# Blue
color4=   #86a2be
color12=  #98afd9

# Magenta
color5=   #d9b798
color13=  #d7d998

# Cyan
color6=   #a1b5cd
color14=  #a1b5cd

# White
color7=   #ffffff
color15=  #dedede

